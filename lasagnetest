/** * Determines the number of minutes the lasagna still needs to remain in the * oven to be properly prepared.*@param {number} actualMinutesInOven ** @param {number} preparationTime * @returns {number} the number of minutes remaining */
function remainingMinutesInOven(actualMinutesInOven, preparationTime) 
{  
    return preparationTime - actualMinutesInOven;
}

/** * Given a number of layers, determines the total preparation time. * 
 * * @param {number} numberOfLayers 
 * * @returns {number} the total preparation time */
function preparationTimeInMinutes(numberOfLayers) 
{  
  const PREPARATION_MINUTES_PER_LAYER = 2;  
  return numberOfLayers * PREPARATION_MINUTES_PER_LAYER;
}
